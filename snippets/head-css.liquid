<style>
  @font-face {
    font-family: 'GT-Walsheim';
    font-display: swap;
    src: url("{{ 'GT-Walsheim-Condensed-Regular.eot' | asset_url }}") format('eot'),
         url("{{ 'GT-Walsheim-Condensed-Regular.woff2' | asset_url }}") format('woff2'),
         url("{{ 'GT-Walsheim-Condensed-Regular.woff' | asset_url }}") format('woff');
    font-weight: normal;
    font-style: normal;
  }

  @font-face {
    font-family: 'GT-Walsheim';
    font-display: swap;
    src: url("{{ 'GT-Walsheim-Condensed-Regular-Oblique.eot' | asset_url }}") format('eot'),
         url("{{ 'GT-Walsheim-Condensed-Regular-Oblique.woff2' | asset_url }}") format('woff2'),
         url("{{ 'GT-Walsheim-Condensed-Regular-Oblique.woff' | asset_url }}") format('woff');
    font-weight: normal;
    font-style: italic;
  }

  @font-face {
    font-family: 'GT-Walsheim';
    font-display: swap;
    src: url("{{ 'GT-Walsheim-Condensed-Bold.eot' | asset_url }}") format('eot'),
         url("{{ 'GT-Walsheim-Condensed-Bold.woff2' | asset_url }}") format('woff2'),
         url("{{ 'GT-Walsheim-Condensed-Bold.woff' | asset_url }}") format('woff');
    font-weight: 700;
    font-style: normal;
  }

  @font-face {
    font-family: 'GT-Walsheim';
    font-display: swap;
    src: url("{{ 'GT-Walsheim-Condensed-Bold-Oblique.eot' | asset_url }}") format('eot'),
         url("{{ 'GT-Walsheim-Condensed-Bold-Oblique.woff2' | asset_url }}") format('woff2'),
         url("{{ 'GT-Walsheim-Condensed-Bold-Oblique.woff' | asset_url }}") format('woff');
    font-weight: 700;
    font-style: italic;
  }
  .ju_controls_btn,
  #preview-bar-iframe {display: none !important}
</style>

